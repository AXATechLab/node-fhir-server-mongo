{
  "name": "@asymmetrik/node-fhir-server-mongo",
  "version": "0.0.2",
  "description": "Mongo Server implementing @asymmetrik/node-fhir-server-core",
  "main": "src/index.js",
  "repository": "https://github.com/Asymmetrik/node-fhir-server-mongo.git",
  "contributors": [
    "Robert Winterbottom <rwinterbottom@asymmetrik.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "start": "export NODE_ENV=production; node src/index.js;",
    "nodemon": "export NODE_ENV=development; node scripts/nodemon;",
    "test": "set -e; npm run test:lint; npm run test:jest;",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:jest": "jest",
    "test:prepare-db": "node scripts/populate",
    "populate": "node scripts/populate"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/testutils/"
    ]
  },
  "dependencies": {
    "@asymmetrik/node-fhir-server-core": "git+https://github.com/Asymmetrik/node-fhir-server-core.git#develop",
    "jsonwebtoken": "^8.1.0",
    "moment-timezone": "^0.5.14",
    "mongodb": "^3.0.1",
    "var": "^0.3.2"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "jest": "^22.0.4",
    "nodemon": "^1.14.7"
  },
  "private": false
}
