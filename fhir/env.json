{
	"MONGO_HOSTNAME": {
		"type": "string",
		"default": "localhost:27017",
		"required": true
	},
	"MONGO_DB_NAME": {
		"type": "string",
		"default": "mongo-on-fhir",
		"required": true
	},
	"LOGGING_LEVEL": {
		"type": "string",
		"default": "debug"
	},
	"SERVER_PORT": {
		"type": "number",
		"default": 3000
	},
	"SSL_KEY": {
		"type": "string",
		"default": "./src/key.pem"
	},
	"SSL_CERT": {
		"type": "string",
		"default": "./src/cert.pem"
	}
}
